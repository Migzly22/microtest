{
    "version": 3,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/public/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/public/font/(.*)",
            "dest": "/public/font/$1"
        }
    ]
}